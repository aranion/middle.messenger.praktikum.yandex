{
  "name": "ya_messenger",
  "version": "1.0.0",
  "description": "messenger",
  "scripts": {
    "start": "parcel -p 3000 ./static/index.hbs --open",
    "go": "npx eslint . --fix && parcel -p 3000 ./static/index.hbs --open",
    "eslint": "npx eslint .",
    "lint:sass": "stylelint src/**/*.sass",
    "lint:sass:fix": "stylelint src/**/*.sass --fix",
    "build": "parcel build ./static/index.hbs",
    "server": "set PORT=3000 && node ./server/index.ts",
    "serverLinux": "PORT=3000 node ./server/index.ts"
  },
  "engines": {
    "npm": "8.15.0",
    "node": "16.13.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.1"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@parcel/transformer-sass": "2.7.0",
    "@types/express": "4.17.13",
    "@types/node": "18.7.3",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "parcel": "2.7.0",
    "stylelint": "14.11.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0"
  }
