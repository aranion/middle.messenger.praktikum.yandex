{
  "name": "ya_messenger",
  "version": "1.0.0",
  "description": "messenger",
  "scripts": {
    "start": "parcel -p 3000 ./static/index.hbs --open",
    "build": "npm run rmdir -- .parcel-cache dist && parcel build ./static/index.hbs",
    "server": "node ./server/index.ts",
    "rmdir": "node -e \"var fs = require('fs'); process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true })); process.exit(0);\""
  },
  "engines": {
    "npm": "8.15.0",
    "node": "16.13.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "^1.0.9",
    "@parcel/transformer-sass": "^2.7.0",
    "@types/node": "^18.7.3",
    "parcel": "^2.7.0"
  }
}