{
  "name": "ya_messenger",
  "version": "1.0.0",
  "description": "messenger",
  "scripts": {
    "start": "rm -rf .parcel-cache && rm -rf dist  && parcel -p 3000 ./static/index.html --open",
    "go": "npx eslint . --fix && parcel -p 3000 ./static/index.html --open",
    "eslint": "npx eslint .",
    "eslint:fix": "npx eslint . --fix",
    "lint": "stylelint src/**/*.{css,sass}",
    "lint:fix": "stylelint src/**/*.{css,sass} --fix",
    "build": "parcel build ./static/index.hbs",
    "server": "set PORT=3000 && node ./server/index.ts",
    "serverLinux": "PORT=3000 node ./server/index.ts"
  },
  "engines": {
    "npm": "8.15.0",
    "node": "16.13.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.1",
    "nanoid": "4.0.0",
    "postcss-sass": "0.5.0"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@parcel/transformer-sass": "2.7.0",
    "@types/express": "4.17.13",
    "@types/node": "18.7.3",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "parcel": "2.7.0",
    "stylelint": "14.11.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0",
    "typescript": "4.7.4"
  }
}
